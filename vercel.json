{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "cleanUrls": true,
  "framework": "nextjs",
  "buildCommand": "cd frontend-web && npm install && npm run build",
  "installCommand": "cd frontend-web && npm install",
  "outputDirectory": "frontend-web/.next",
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/frontend-web/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}